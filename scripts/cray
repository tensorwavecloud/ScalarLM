# :command.usage
cray_usage() {
  printf "cray - Cray CLI\n\n"

  printf "%s\n" "Usage:"
  printf "  cray COMMAND\n"
  printf "  cray [COMMAND] --help | -h\n"
  printf "  cray --version | -v\n"
  echo
  # :command.usage_commands
  printf "%s\n" "Commands:"
  printf "  %s   Build image from dockerfile\n" "build-image"
  printf "  %s   Build image from dockerfile and push to depot\n" "depot-build"
  printf "  %s   Start the container\n" "up         "
  printf "  %s   Run tests in the container\n" "test       "
  printf "  %s   Deploy the cray platform to modal\n" "deploy     "
  printf "  %s   Serve the cray platform using modal\n" "serve      "
  echo

  # :command.long_usage
  if [[ -n "$long_usage" ]]; then
    printf "%s\n" "Options:"

    # :command.usage_fixed_flags
    printf "  %s\n" "--help, -h"
    printf "    Show this help\n"
    echo
    printf "  %s\n" "--version, -v"
    printf "    Show version number\n"
    echo

  fi
}

